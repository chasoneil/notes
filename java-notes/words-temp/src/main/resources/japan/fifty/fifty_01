
// 五十音图
/*
A # B # C
A: 平假名
B: 片假名
C: 读音（键盘拼写）罗马音
*/

あ#ア#a
い#イ#i
う#ウ#u
え#エ#e
お#オ#o

か#カ#ka
き#キ#ki
く#ク#ku
け#ケ#ke
こ#コ#ko

さ#サ#sa
し#シ#shi:si
す#ス#su
せ#セ#se
そ#ソ#so

た#タ#ta
ち#チ#chi:ti
つ#ツ#tsu:tu
て#テ#te
と#ト#to

な#ナ#na
に#ニ#ni
ぬ#ヌ#nu
ね#ネ#ne
の#ノ#no

は#ハ#ha
ひ#ヒ#hi
ふ#フ#hu:fu
へ#ヘ#he
ほ#ホ#ho

ま#マ#ma
み#ミ#mi
む#ム#mu
め#メ#me
も#モ#mo

や#ヤ#ya
ゆ#ユ#yu
よ#ヨ#yo

ら#ラ#ra
り#リ#ri
る#ル#ru
れ#レ#re
ろ#ロ#ro

わ#ワ#wa
を#ヲ#wo:o

ん#ン#n:ng:m
